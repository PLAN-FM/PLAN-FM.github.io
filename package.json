{
  "name": "PLAN-FM",
  "homepage": "https://plan-fm.github.io",
  "version": "0.0.0",
  "repository": "git@github.com:PLAN-FM/PLAN-FM.git",
  "bugs": "",
  "license": "MIT",
  "scripts": {
    "build": "react-scripts build",
    "clean": "yarn cache clean && yarn install",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "start": "react-scripts start",
    "predeploy": "yarn build",
    "deploy": "yarn build && gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@carbon/icons-react": "^11.55.0",
    "@carbon/react": "^1.31.3",
    "react": "^18.1.0",
    "react-autosuggest": "^10.0.2",
    "react-dom": "^18.1.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.51.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "gh-pages": "^5.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.13.1",
    "prettier": "^1.17.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
